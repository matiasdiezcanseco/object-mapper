{
  "name": "@matiasdc/object-mapper",
  "version": "1.0.0",
  "description": "A TypeScript package for mapping and validating objects with a simple API. It uses Zod for validation and Lodash for deep cloning and merging.",
  "keywords": [
    "object",
    "typescript",
    "mapper",
    "mapping",
    "utility"
  ],
  "homepage": "https://github.com/matiasdiezcanseco/package-name",
  "bugs": {
    "url": "https://github.com/matiasdiezcanseco/package-name/issues"
  },
  "author": "Matias Diez Canseco <matiasdiezcanseco@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matiasdiezcanseco/package-name.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "ci": "npm run build && npm run check-format && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "vitest run",
    "dev": "vitest"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@zod/core": "^0.11.6",
    "lodash": "^4.17.21",
    "zod": "^4.0.14"
  }
}
